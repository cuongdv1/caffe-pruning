train_net: "models/VGG16/mnc_5stage/train.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 20000
display: 100
average_loss: 100
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
snapshot_prefix: "vgg16_mnc_5stage"
iter_size: 8


